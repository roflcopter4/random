# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Assignment1_config = debug
endif
ifeq ($(config),release)
  Assignment1_config = release
endif

PROJECTS := Assignment1

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Assignment1:
ifneq (,$(Assignment1_config))
	@echo "==== Building Assignment1 ($(Assignment1_config)) ===="
	@${MAKE} --no-print-directory -C . -f Assignment1.make config=$(Assignment1_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Assignment1.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Assignment1"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"